@font-face {
	font-family: 'Nanum'; /* 폰트 패밀리 이름 주기*/
	src: url('../fonts/NanumGothicExtraBold.ttf'); /*폰트 파일 주소*/
}

@font-face {
	font-family: 'norwester'; /* 폰트 패밀리 이름 주기*/
	src: url('../fonts/norwester.otf'); /*폰트 파일 주소*/
}

#sideMenu {
	width: 205px;
	height: auto;
	position: relative;
	border-right: 1px solid #acacad;
	border-left: 1px solid #acacad;
	float: left;
}

#trainTitle {
	display: table;
	width: 100%;
	height: 100px;
	font-size: 20px;
	font-weight: bold;
	backgraund-color: #1155cc;
	color: #3a3838;
	font-family: 'Nanum' !important;
}

#trainTitle p {
	display: table-cell;
	text-align: center;
	vertical-align: middle;
}

#traingMenu {
	display: table;
	background-color: #1155cc;
	width: 100%;
	height: 50px;
	color: white;
	font-family: 'Nanum' !important;
	font-weight: bold;
	vertical-align: middle;
	font-size: 13px;
	box-shadow: 1px 3px 3px #888888;
}

#traingMenu p {
	display: table-cell;
	text-align: left;
	padding-left: 10px;
	vertical-align: middle;
}

#traingMenu1 {
	display: table;
	background-color: white;
	width: 100%;
	height: 50px;
	font-family: 'Nanum' !important;
	font-size: 11px;
	vertical-align: middle;
	border-bottom: 1px solid #acacad;
}

#traingMenu1 p {
	display: table-cell;
	text-align: left;
	padding-left: 10px;
	vertical-align: middle;
}

#traingMenu2 {
	display: table;
	background-color: white;
	width: 100%;
	height: 50px;
	color: #575757;
	font-family: 'Nanum' !important;
	font-weight: bold;
	font-size: 11px;
	vertical-align: middle;
	border-bottom: 1px solid #acacad;
}

#traingMenu2 p {
	display: table-cell;
	text-align: left;
	padding-left: 10px;
	vertical-align: middle;
}

#traingMenu1 a {
	text-decoration: none;
	color: #525252;
}

