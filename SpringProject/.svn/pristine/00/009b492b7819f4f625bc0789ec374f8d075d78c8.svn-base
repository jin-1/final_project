}

#tarinInfoMain {
	width: 100%;
	position: releative;
	height: auto;
	float: right;
	left: 230px;
	top: 30px;
}

#trainName {
	width: 100%;
}

#trainName span {
	font-family: 'Nanum' !important;
	font-size: 30px;
	color: #2f2f2f;
	font-weight: bold;
}

#trainInfoMenuTable {
	width: 100%;
	height: 650px;
	font-family: 'Nanum' !important;
	text-align: center;
}

#trainInfoMenuTable th {
	background: #f8f8f8;
}

#trainInfoMenuTableR {
	width: 100%;
	width: 100%;
	border-top: 2px solid #0097d0;
	border-bottom: 2px solid #b6b6b6;
	font-size: 12px;
}

#trainInfoMenuTableR th {
	height: 20px;
	background: #f8f8f8 url(../img/bar_th.gif) no-repeat left 50%;
}

#trainInfoMenuTableR td {
	height: 30px;
	border-bottom: 1px solid #b6b6b6;
}

