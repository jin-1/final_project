.paging {
	position: relative;
	width: 100%;
	height: auto;
	top: 30px;
	text-align: center;
}

.pagination {
	list-style: none;
	margin: 0;
	padding: 0 5px;
}

.pagination li {
	display: inline;
	padding: 4px;
	margin-right: 3px;
	width: 40px;
	color: #000;
	font: bold 13px tahoma;
	border: 1px solid #eee;
	text-align: center;
	text-decoration: none;
}

.location {
	font-family: 'Nanum' !important;
	font-size: 13px;
	font-weight: bold;
	color: #525252;
}

.location a {
	text-decoration: none;
	color: #525252;
}

.location a:visited {
	text-decoration: none;
	color: #525252;
